<launch>
  <param name="robot_description" textfile="$(find justina_description)/urdf/justina.urdf" />
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="debug" value="false"/>
    <arg name="world_name" value="$(find gazebo_envs)/worlds/appartment.world"/>
  </include>
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-param robot_description -urdf -model justina -x 5.2 -y 3.8 -z 0.03 -Y 3.141592"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find config_files)/rviz/justina.rviz"/>
  <!--node name="simple_gui" pkg="simple_gui" type="simple_gui_node"/-->
  <node name="mobile_base" pkg="mobile_base_simul" type="mobile_base_simul.py" output="screen"/>
  <node name="map_server" pkg="map_server" type="map_server" output="screen" args="$(find config_files)/maps/appartment.yaml"/>
  <node name="amcl" pkg="amcl" type="amcl" output="screen"/>
</launch>
